itions.aspx?ID=19\">frightened 1</a> (<a style=\"text-decoration:underline\" href=\"Conditions.aspx?ID=19\">frightened 2</a> on a critical failure). If the blood boar uses Terrifying Squeal on the same turn after using Chase Down, the Will save DC is instead 25.","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["auditory","emotion","fear","mental"]},"actionType":{"value":"action"},"actionCategory":{"value":""},"weapon":{"value":""},"actions":{"value":1},"requirements":{"value":""},"trigger":{"value":""},"skill_requirements":{"skill":"","rank":""}},"sort":600000},{"_id":"HAlI3oChDIEeHIfV","flags":{},"name":"Acrobatics","type":"lore","img":"icons/svg/mystery-man.svg","data":{"description":{"value":"","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":[]},"featType":"","mod":{"value":10},"proficient":{"value":0},"item":{"value":0}},"sort":700000},{"_id":"LWHqpf7sFIr872gB","flags":{},"name":"Athletics","type":"lore","img":"icons/svg/mystery-man.svg","data":{"description":{"value":"","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":[]},"featType":"","mod":{"value":15},"proficient":{"value":0},"item":{"value":0}},"sort":800000},{"_id":"2v2iun8vSPydWMrt","flags":{},"name":"Survival","type":"lore","img":"icons/svg/mystery-man.svg","data":{"description":{"value":"","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":[]},"featType":"","mod":{"value":15},"proficient":{"value":0},"item":{"value":0}},"sort":900000}]}
