<form class="{{cssClass}} crb-style" autocomplete="off" spellcheck="off">
    <aside>
        <div class="sidebar" data-group="dashboard" data-tab="sidebar">
            {{> "systems/pf2e/templates/actors/siege-weapon/sidebar/health.hbs"}}
            {{> "systems/pf2e/templates/actors/siege-weapon/sidebar/armorclass.hbs"}}
            {{> "systems/pf2e/templates/actors/siege-weapon/sidebar/saves.hbs"}}
            {{> "systems/pf2e/templates/actors/siege-weapon/sidebar/iwr.hbs"}}
        </div>
    </aside>
    {{> "systems/pf2e/templates/actors/siege-weapon/header.hbs"}}
    <nav class="sheet-navigation" data-group="primary">
        <span class="navigation-title">{{localize "PF2E.TabDetailsLabel"}}</span>
        <a class="item" data-tab="details" title="{{localize "PF2E.TabDetailsLabel"}}"><i class="fas fa-dharmachakra"></i></a>
        <a class="item" data-tab="description" title="{{localize "PF2E.TabDescriptionLabel"}}"><i class="fas fa-book-reader"></i></a>
    </nav>
    <section class="sheet-body">
        <section class="sheet-content">
            {{> "systems/pf2e/templates/actors/siege-weapon/tabs/details.hbs"}}
            {{> "systems/pf2e/templates/actors/siege-weapon/tabs/description.hbs"}}
        </section>
    </section>
</form>
