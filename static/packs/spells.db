{"_id":"mrDi3v933gsmnw25","name":"Telekinetic Maneuver","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>With a rush of telekinetic power, you move a foe or something they carry. You can attempt to Disarm, Shove, or Trip the target using a spell attack roll instead of an Athletics check.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=333","type":"String","label":"Source"},"traits":{"value":["attack","evocation","force"],"type":"String","label":"Spell Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":2,"type":"Number","label":"Spell Level"},"spellType":{"value":"attack","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","occult"],"type":"String","label":"Spell Traditions"},"school":{"value":"evo","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"1 creature","type":"String","label":"Target"},"range":{"value":"60 feet","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":"","type":"String","label":"Spellcasting Ent