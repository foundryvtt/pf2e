{"_id":"kl2q6JvBZwed4B6v","name":"Dancing Lights","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>You create up to four floating lights, no two of which are more than 10 feet apart. Each sheds light like a torch. When you Sustain the Spell, you can move any number of lights up to 60 feet. Each light must remain within 120 feet of you and within 10 feet of all others, or it winks out.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":["light","cantrip","evocation"],"type":"String","label":"Spell Traits","custom":""},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":0,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","occult","primal"],"type":"String","label":"Spell Traditions","custom":""},"school":{"value":"evo","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"","type":"String","label":"Target"},"range":{"value":"120 feet","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"sustained","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":true,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"}