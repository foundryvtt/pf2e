{"_id":"W69zswpj0Trdy5rj","name":"Air Bubble","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>A bubble of pure air appears around the target's head, allowing it to breathe normally. The effect ends as soon as the target returns to an environment where it can breathe normally.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=5","type":"String","label":"Source"},"traits":{"value":["air","conjuration"],"type":"String","label":"Spell Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":1,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","divine","primal"],"type":"String","label":"Spell Traditions"},"school":{"value":"con","type":"String","label":"Spell School"},"components":{"value":"verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"the triggering creature","type":"String","label":"Target"},"range":{"value":"60 feet","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"reaction","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":147,"type":"String","label":"Spellcasting Ent