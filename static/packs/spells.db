{"_id":"9AAkVUCwF6WVNNY2","name":"Lightning Bolt","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>A bolt of lightning strikes outward from your hand, dealing 4d12 electricity damage.</p>\n<hr>\n<p><strong>Heightened (+1)</strong> The damage increases by 1d12.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=172","type":"String","label":"Source"},"traits":{"value":["electricity","evocation"],"type":"String","label":"Spell Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":3,"type":"Number","label":"Spell Level"},"spellType":{"value":"save","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","primal"],"type":"String","label":"Spell Traditions"},"school":{"value":"evo","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"","type":"String","label":"Target"},"range":{"value":"","type":"String","label":"Range"},"area":{"value":120,"type":"String","label":"Area","areaType":"line"},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"4d12","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"electricity","type":"String","label":"Damage Type"},"scaling":{"mode":"level1","formula":"1d12"},"save":{"basic":"basic","type":"String","label":"Saving Throw","value":"reflex"},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":"","type":"String","label":"Spellcasting Entry Location"},"