{"_id":"ihbRf964JDXztcy3","name":"Disjunction","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DetailedOutput\">Crackling energy disjoins the target. You attempt to counteract it. If you succeed, it's deactivated for 1 week. On a critical success, it's destroyed. If it's an artifact or similar item, you automatically fail.&nbsp;</span></p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=77","type":"String","label":"Source"},"traits":{"value":["uncommon","abjuration"],"type":"String","label":"Spell Traits","custom":"","rarity":{"value":"uncommon"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":9,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","primal"],"type":"String","label":"Spell Traditions"},"school":{"value":"abj","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"1 magic item","type":"String","label":"Target"},"range":{"value":"120 feet","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String"