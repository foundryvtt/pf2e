{"_id":"KcLVELhCUcKXxiKE","name":"Longstrider","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>You lengthen your stride beyond what should be possible. You gain a +10-foot status bonus to your Speed.</p>\n<hr>\n<p><strong>Heightened (2nd)</strong> The duration increases to 8 hours.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=175","type":"String","label":"Source"},"traits":{"value":["transmutation"],"type":"String","label":"Spell Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":1,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","primal"],"type":"String","label":"Spell Traditions"},"school":{"value":"trs","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"","type":"String","label":"Target"},"range":{"value":"","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"1 hour","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":"","type":"String","label":"Spellcasting Entry Location"},"spellCategori