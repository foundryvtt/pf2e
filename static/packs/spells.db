{"_id":"Hu38hoAUSYeFpkVa","name":"Force Bolt","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>You fire an unerring dart of force from your fingertips. It automatically hits and deals 1d4+1 force damage to the target.</p>\n<hr>\n<p><strong>Heightened (+2)</strong> The damage increases by 1d4+1.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=529","type":"String","label":"Source"},"traits":{"value":["uncommon","force","evocation","wizard"],"type":"String","label":"Spell Traits","custom":"","rarity":{"value":"uncommon"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":1,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["focus"],"type":"String","label":"Spell Traditions"},"school":{"value":"evo","type":"String","label":"Spell School"},"components":{"value":"somatic","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"1 creature or object","type":"String","label":"Target"},"range":{"value":"30 feet","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"1","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"1d4+1","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"force","type":"String","label":"Damage Type"},"scaling":{"mode":"level2","formula":"1d4+1"},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":""