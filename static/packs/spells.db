{"_id":"lY9fOk1qBDDhBT8s","name":"Protective Ward","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>You emanate a shimmering aura of protective magic. You and any allies in the area gain a +1 status bonus to AC. Each time you Sustain the Spell, the emanation's radius increases by 5 feet, to a maximum of 30 feet.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=534","type":"String","label":"Source"},"traits":{"value":["abjuration","wizard"],"type":"String","label":"Spell Traits","rarity":{"value":"uncommon"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":1,"type":"Number","label":"Spell Level"},"spellType":{"value":"utility","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["focus"],"type":"String","label":"Spell Traditions"},"school":{"value":"abj","type":"String","label":"Spell School"},"components":{"value":"somatic","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"","type":"String","label":"Target"},"range":{"value":"","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"1","type":"String","label":"Casting Time"},"duration":{"value":"sustained up to 1 minute","type":"String","label":"Duration"},"damage":{"value":"","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"","type":"String","label":"Damage Type"},"scaling":{"mode":"none","formula":""},"save":{"basic":"","type":"String","label":"Saving Throw","value":""},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":"","type":"String","label":"Spellc