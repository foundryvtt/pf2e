{"_id":"3puDanGfpEt6jK5k","name":"Cone of Cold","permission":{"default":0},"type":"spell","data":{"description":{"value":"<p>Icy cold rushes forth from your hands. You deal 12d6 cold damage to creatures in the area.</p>\n<hr>\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"https://2e.aonprd.com/Spells.aspx?ID=47","type":"String","label":"Source"},"traits":{"value":["cold","evocation"],"type":"String","label":"Spell Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"level":{"value":5,"type":"Number","label":"Spell Level"},"spellType":{"value":"save","type":"String","label":"Spell Type"},"spellCategory":{"value":""},"traditions":{"value":["arcane","primal"],"type":"String","label":"Spell Traditions"},"school":{"value":"evo","type":"String","label":"Spell School"},"components":{"value":"somatic, verbal","type":"String","label":"Spell Components"},"materials":{"value":"","type":"String","label":"Materials"},"target":{"value":"","type":"String","label":"Target"},"range":{"value":"","type":"String","label":"Range"},"area":{"value":null,"type":"String","label":"Area","areaType":""},"time":{"value":"2","type":"String","label":"Casting Time"},"duration":{"value":"","type":"String","label":"Duration"},"damage":{"value":"12d6","applyMod":false,"type":"String","label":"Spell Damage"},"damageType":{"value":"cold","type":"String","label":"Damage Type"},"scaling":{"mode":"level1","formula":"2d6"},"save":{"basic":"basic","type":"String","label":"Saving Throw","value":"reflex"},"sustained":{"value":false,"type":"Boolean","label":"Can be Sustained"},"cost":{"value":"","type":"String","label":"Cost"},"ability":{"value":"","type":"String","label":"Spellcasting Ability"},"prepared":{"value":"","type":"String","label":"Prepared Spell"},"location":{"value":"","type":"String","label":"Spellcasting Entry Location"},"spellCategorie":