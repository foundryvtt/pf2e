{"_id":"qK964kZz1ALcysOa","name":"Elixir of Life (True)","permission":{"default":0},"type":"consumable","data":{"description":{"value":"Elixirs of life accelerate the body’s natural healing processes and immune system. Upon drinking this elixir, you regain the listed number of Hit Points and gain an item bonus to saving throws against diseases and poisons for 10 minutes. The elixir restores 10d6+27 Hit Points, and the bonus is +4.","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":["alchemical","consumable","elixir","healing"],"custom":"","rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"L","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"3,000 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null,"autoUse":true,"autoDestroy":true},"level":{"value":19,"type":"Number","label":"Item Level"},"consumableType":{"value":"potion","type":"String","label":"Consumable Type"},"charges":{"value":1,"max":1,"_deprecated":true,"type":"Number","label":"Charges"},"consume":{"value":"10d6+27","_deprecated":true,"type":"String","label":"Roll on Consume"},"autoUse":{"value":true,"_deprecated":true,"type":"Boolean","label":"Consume on Use"},"autoDestr