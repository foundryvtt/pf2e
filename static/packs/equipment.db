{"_id":"2prxM8Q0F4sdSwPx","name":"Winter Wolf Elixir (Moderate)","permission":{"default":0},"type":"consumable","data":{"description":{"value":"This elixir warms your core and improves your circulation. For 24 hours, you are protected from the effects of severe cold. You’re also protected from extreme cold.","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":[]},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"L","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"320 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null,"autoUse":true,"autoDestroy":true},"level":{"value":12,"type":"Number","label":"Item Level"},"consumableType":{"value":"potion","type":"String","label":"Consumable Type"},"charges":{"value":1,"max":1,"_deprecated":true,"type":"Number","label":"Charges"},"consume":{"value":"","_deprecated":true,"type":"String","label":"Roll on Consume"},"autoUse":{"value":true,"_deprecated":true,"type":"Boolean","label":"Consume on Use"},"autoDestroy":{"value":true,"_deprecated":true,"type":"Boolean","label":"Destroy on Empty"}},"sort":9500001,"flags":{},"img":"systems/pf2e/icons/equipment/alchemical-items/alchemical-elixirs/winter-wolf-elixir.jpg"}
{"_id":"2req0jGaxz8hScdB","name":"Adventurer's Pack","permission":{"default":0,"Ru8DFixrPJBEbf4X":3},"type":"kit","data":{"description":{"value":"<p style=\"box-sizing: border-box; user-select: text; color: #191813; font-family: Roboto, system, -apple-system, sans-serif; font-size: 13px;\"><span style=\"box-sizing: border-box; user-select: text; display: block;\">This item is the starter kit for an adventurer, containing the essential items for exploration and survival. The Bulk value is for the entire pack together, but see the descriptions of individual items as necessary.</span></p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":[],"rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"items":{"mca3x":{"pack":"pf2e.equipment-srd","id":"3lgwjrFEsQVKzhh7","img":"systems/pf2e/icons/equipment/adventuring-gear/backpack.jpg","quantity":1,"name":"Backpack","isContainer":true,"items":{"lpl11":{"pack":"pf2e.equipment-srd","id":"fagzYdmfYyMQ6J77","img":"systems/pf2e/icons/equipment/adventuring-gear/bedroll.jpg","quantity":1,"name":"Bedroll","isContainer":false,"items":{}},"afbn4":{"pack":"pf2e.equipment-srd","id":"xShIDyydOMkGvGNb","img":"systems/pf2e/icons/equipment/adventuring-gear/chalk.jpg","quantity":10,"name":"Chalk","isContainer":false,"items":{}},"fabyb":{"pack":"pf2e.equipment-srd","id":"UlIxxLm71UdRgCFE","img":"systems/pf2e/icons/equipment/adventuring-gear/flint-and-steel.jpg","quantity":1,"name":"Flint and Steel","isContainer":false,"items":{}},"02xhi":{"pack":"pf2e.equipment-srd","id":"fyYnQf1NAx9fWFaS","img":"systems/pf2e/icons/equipment/adventuring-gear/rope.jpg","quantity":1,"name":"Rope","isContainer":false,"items":{}},"jtagt":{"pack":"pf2e.equipment-srd","id":"L9ZV076913otGtiB","img":"systems/pf2e/icons/equipment/adventuring-gear/rations.jpg","quantity":14,"name":"Rations","isContainer":false,"items":{}},"lems2":{"pack":"pf2e.equipment-srd","id":"8Jdw4yAzWYylGePS","img":"systems/pf2e/icons/equi