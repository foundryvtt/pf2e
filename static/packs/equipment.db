{"_id":"e4NwsnPnpQKbDZ9F","name":"Composite Shortbow","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p><span class=\"fontstyle0\">This shortbow is made from horn, wood, and sinew laminated together to increase the power of its pull and the force of its projectile. Its compact size and power make it a favorite of mounted archers. Any time an ability is specifically restricted to a shortbow, it also applies to composite shortbows unless otherwise stated.</span> </p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":["deadly-d10","propulsive"],"type":"String","label":"Weapon Traits","custom":"","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"1","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"14 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":0,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"criticalConditionType":"","criticalConditionValue":null},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Secondary Property"},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDam