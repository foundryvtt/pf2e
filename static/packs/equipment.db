{"_id":"ewQZ0VeL38v3qFnN","name":"Studded Leather Armor","permission":{"default":0},"type":"armor","data":{"description":{"value":"<p><span class=\"fontstyle0\">This leather armor is reinforced with metal studs and sometimes small metal plates, providing most of the flexibility of leather armor with more robust protection.</span> </p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":"","type":"String","label":"Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":null},"maxHp":{"value":null},"hardness":{"value":null},"brokenThreshold":{"value":null},"weight":{"value":"2","type":"Number","label":"Bulk"},"equippedBulk":{"value":"1"},"price":{"value":"3 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":true,"type":"Boolean","label":"Equipped"},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":0,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 1"},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 2"},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 3"},"armor":{"value":2,"type":"Number","label":"AC Bonus"},"armorType":{"value":"light","type":"String","label":"Armor Category"},"group":{"value":"leather","type":"String","label":"Group"},"strength":{"value":"12","type":"Stri