{"_id":"f1gwoTkf3Nn0v3PN","name":"Whip","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p>This long strand of thick leather, often braided, delivers a painful but nonlethal slash at a distance, usually accompanied by a distinctive cracking sound.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":["disarm","finesse","nonlethal","reach","trip"],"type":"String","label":"Weapon Traits","custom":"","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"1","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"1 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":null,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"criticalConditionType":"","criticalConditionValue":null},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Secondary Property"},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Tertiary Property"},"weaponType":{"value":"martial","type":"String","label":"Weapon Category"},"group":{"value":"flail","type":"String","label":"Weapon Group"},