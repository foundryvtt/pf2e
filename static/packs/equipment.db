{"_id":"r0ifJfoz8aqf0mwk","name":"Breastplate","permission":{"default":0},"type":"armor","data":{"description":{"value":"<p><span class=\"fontstyle0\">Though referred to as a breastplate, this type of armor consists of several pieces of plate or half‑plate armor (page 276) that protect the torso, chest, neck, and sometimes the hips and lower legs. It strategically grants some of the protection of plate while allowing greater flexibility and speed.</span></p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":"","type":"String","label":"Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":null},"maxHp":{"value":null},"hardness":{"value":null},"brokenThreshold":{"value":null},"weight":{"value":"3","type":"Number","label":"Bulk"},"equippedBulk":{"value":"2"},"price":{"value":"8 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false,"type":"Boolean","label":"Equipped"},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":0,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 1"},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 2"},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 3"},"armor":{"value":4,"type":"Number","label":"AC Bonus"},"armorType":{"value":"medium","type":"Str