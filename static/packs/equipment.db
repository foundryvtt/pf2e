{"_id":"ZhxxqYpVdVx0jSMm","name":"Shield Bash","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p>A shield bash is not actually a weapon, but a maneuver in which you thrust or swing your shield to hit your foe with an impromptu attack.</p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":[],"type":"String","label":"Weapon Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"-","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":null,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Primary Property","strikeConditionType":"","strikeConditionValue":null,"criticalConditionType":"","criticalConditionValue":null},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Secondary Property"},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"String","label":"Tertiary Property"},"weaponType":{"value":"martial","type":"String","label":"Weapon Category"},"group":{"value":"shield","type":"String","label":"Weapon Group"},"hands":{"value":"","type":"String","label":"Hands"},"bonus":{"value":"0