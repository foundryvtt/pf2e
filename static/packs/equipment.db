{"_id":"z1okOYtNVnpBNj9F","name":"Religious Text","permission":{"default":0},"type":"equipment","data":{"description":{"value":"<p><span class=\"fontstyle0\">This manuscript contains scripture of a particular religion. Some divine spellcasters, such as clerics, can use a religious text as a divine focus to use certain abilities and cast some spells. A religious text must be held in one hand to use it.</span> </p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":[]},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"L","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"1 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false,"type":"Boolean","label":"Equipped"},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":null,"type":"Number","label":"Item Level"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/equipment/adventuring-gear/religious-text.jpg"}
