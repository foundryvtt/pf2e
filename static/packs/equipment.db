{"_id":"NufKswitJjxDCX8f","name":"Naginata","permission":{"default":0,"T6ru9BggWhvbGU3S":3},"type":"weapon","data":{"description":{"value":"<p><span>This 6-foot staff has a 2-foot-long, slightly curved, swordlike blade attached at one end. The long pole helps keep the wielder out of reach of swords and shorter weapons.&nbsp;</span></p>","chat":"","unidentified":""},"source":{"value":""},"traits":{"value":["uncommon","deadly-d8","reach","versatile-p"],"custom":"","rarity":{"value":"uncommon"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":"1"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"2"},"equippedBulk":{"value":""},"price":{"value":"3 gp"},"invested":{"value":false},"equipped":{"value":false},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":"0"},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":null,"critDie":"","critDamage":"","critDamageType":"","strikeConditionType":"","strikeConditionValue":null,"criticalConditionType":"","criticalConditionValue":null},"property2":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":""},"property3":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":""},"weaponType":{"value":"martial"},"group":{"value":"polearm"},"hands":{"value":""},"bonus":{"value":0},"damage":{"value":"","dice":1,"die":"d8","damageType":"slashing"},"bonusDamage":{"value":0},"splashDamage":{"value":null},"range":{"value":""},"reload":{"value":""},"ability":{"value":"str"},"MAP":{"value":""},"potencyRune":{"value":""},"strikingRune":{"value":""},"propertyRune1":{"value":""},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"pr