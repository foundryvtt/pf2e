{"_id":"ltundBNFAnP7bgPr","name":"Tower Shield","permission":{"default":0},"type":"armor","data":{"description":{"value":"<p><span class=\"fontstyle0\">These massive shields can be used to provide cover to nearly the entire body. Due to their size, they are typically made of wood reinforced with metal.</span> AC increases to +4 if you are using the Take Cover action.</p>\n<table style=\"height: 32px;\" border=\"1\">\n<tbody>\n<tr style=\"height: 15px;\">\n<td style=\"height: 15px; width: 115px;\">Hardness</td>\n<td style=\"height: 15px; width: 115px;\">HP</td>\n<td style=\"height: 15px; width: 116px;\">BT</td>\n</tr>\n<tr style=\"height: 17px;\">\n<td style=\"height: 17px; width: 115px;\">5</td>\n<td style=\"height: 17px; width: 115px;\">20</td>\n<td style=\"height: 17px; width: 116px;\">10</td>\n</tr>\n</tbody>\n</table>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":"","type":"String","label":"Traits","rarity":{"value":"common"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":20},"maxHp":{"value":20},"hardness":{"value":5},"brokenThreshold":{"value":10},"weight":{"value":"4","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"10 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":true,"type":"Boolean","label":"Equipped"},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":"0"},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":null,"type":"Number","label":"Item Level"},"property1":{"value":"","dice":0,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","type":"Number","label":"Property Rune 1"},"property2":{"value":"","dice":0,"die":"","damageType":"",