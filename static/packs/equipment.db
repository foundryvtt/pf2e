{"_id":"qCEOZ6109Yo34tRx","name":"Formula Book (Blank)","permission":{"default":0},"type":"equipment","data":{"description":{"value":"<p><span class=\"fontstyle0\">A formula book holds the formulas necessary to make items other than the common equipment from this chapter; alchemists typically get one for free. Each formula book can hold the formulas for up to 100 different items. Formulas can also appear on parchment sheets, tablets, and almost any other medium; there’s no need for you to copy them into a specific book as long as you can keep them on hand to reference them.</span> </p>","chat":"","unidentified":"","type":"String","label":"Description"},"source":{"value":"","type":"String","label":"Source"},"traits":{"value":[],"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"}},"rarity":{"value":"common"},"usage":{"value":"held-in-one-hand"},"quantity":{"value":1,"type":"Number","label":"Quantity"},"hp":{"value":0},"maxHp":{"value":0},"hardness":{"value":0},"brokenThreshold":{"value":0},"weight":{"value":"L","type":"Number","label":"Bulk"},"equippedBulk":{"value":""},"price":{"value":"1 gp","type":"String","label":"Price"},"invested":{"value":false},"equipped":{"value":false,"type":"Boolean","label":"Equipped"},"identified":{"value":false},"stackGroup":{"value":""},"bulkCapacity":{"value":""},"negateBulk":{"value":""},"containerId":{"value":""},"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"collapsed":{"value":false},"level":{"value":0,"type":"Number","label":"Item Level"}},"flags":{"_sheetTab":"description"},"img":"systems/pf2e/icons/equipment/adventuring-gear/formula-book.jpg"}
