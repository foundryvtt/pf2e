            "img": "systems/pf2e/icons/equipment/worn-items/other-worn-items/handwraps-of-mighty-blows.webp",
            "name": "+2 Greater Striking Corrosive Handwraps of Mighty Blows",
            "sort": 400000,
            "system": {
                "MAP": {
                    "value": ""
                },
                "baseItem": null,
                "bonus": {
