            "img": "icons/equipment/hand/gauntlet-simple-leather-brown-gold.webp",
            "name": "+3 Major Striking Greater Shock Handwraps of Mighty Blows",
            "sort": 100000,
            "system": {
                "baseItem": null,
                "bonus": {
                    "value": 1
                },
                "bonusDamage": {
