            "img": "systems/pf2e/icons/actions/Passive.webp",
            "name": "Mythic Resilience",
            "sort": 1000000,
            "system": {
                "actionType": {
                    "value": "passive"
                },
                "actions": {
                    "value": null
                },
                "category": "defensive",
                "description": {
                    "value": "<p>Agyra treats her saving throws with Fortitude and Reflex saves as one step better than they actually are (so a critical failure is a failure, a failure is a success, and a success is a critical success). Whenever a character rolls a critical hit against Agyra, they must reroll the attack roll and take the new result.</p>"
                },
                "publication": {
                    "license": "ORC",
                    "remaster": true,
                    "title": "Pathfinder War of Immortals"
                },
                "rules": [
                    {
                        "adjustment": {
