            "img": "icons/equipment/hand/gauntlet-simple-leather-brown-gold.webp",
            "name": "+2 Striking Handwraps of Mighty Blows",
            "sort": 200000,
            "system": {
                "baseItem": null,
                "bonus": {
                    "value": 1
                },
                "bonusDamage": {
