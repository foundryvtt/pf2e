            "img": "systems/pf2e/icons/spells/illusory-disguise.webp",
            "name": "Illusory Disguise",
            "sort": 200000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": null,
                "category": {
                    "value": "spell"
                },
                "components": {
                    "focus": false,
                    "material": false,
