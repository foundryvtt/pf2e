                "publication": {
                    "license": "OGL",
                    "remaster": false,
                    "title": ""
                },
                "rules": [
                    {
                        "key": "EphemeralEffect",
                        "predicate": [
                            "encounter:round:1",
                            {
                                "lt": [
                                    "self:participant:initiative:rank",
                                    "target:participant:initiative:rank"
                                ]
                            },
                            {
                                "or": [
                                    "self:participant:initiative:stat:deception",
                                    "self:participant:initiative:stat:stealth"
