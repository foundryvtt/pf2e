            "img": "systems/pf2e/icons/spells/charm.webp",
            "name": "Charm",
            "sort": 200000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": {
                    "areaType": "",
                    "value": null
                },
                "areasize": {
