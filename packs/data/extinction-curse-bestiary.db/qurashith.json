            "img": "systems/pf2e/icons/spells/teleport.webp",
            "name": "Teleport (At Will, Self Only)",
            "sort": 200000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": null,
                "category": {
                    "value": "spell"
