                },
                "trigger": {
                    "value": ""
                },
                "usage": {
                    "value": "held-in-one-hand"
                },
                "weapon": {
                    "value": ""
                }
            },
            "effects": [],
            "flags": {},
            "img": "systems/pf2e/icons/actions/OneAction.webp",
