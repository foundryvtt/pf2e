            "img": "systems/pf2e/icons/equipment/worn-items/other-worn-items/handwraps-of-mighty-blows.webp",
            "name": "+1 Striking Handwraps of Mighty Blows",
            "sort": 100000,
            "system": {
                "MAP": {
                    "value": ""
                },
                "baseItem": null,
                "bonus": {
                    "value": 1
