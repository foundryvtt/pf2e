        {
            "_id": "kDEtwQUpRYFWo9pJ",
            "img": "systems/pf2e/icons/actions/Passive.webp",
            "name": "Three-Headed",
            "sort": 900000,
            "system": {
                "actionCategory": {
                    "value": "defensive"
                },
                "actionType": {
                    "value": "passive"
                },
                "actions": {
                    "value": null
                },
                "description": {
                    "value": "<p>Any ability that would sever a chimera's head (such as a critical hit with a @UUID[Compendium.pf2e.equipment-srd.Vorpal]{Vorpal} Weapon) severs one head at random. Losing a head doesn't kill a chimera (as long as it has one head left), but it does prevent it from making Strikes with the lost head or using the head's Breath Weapon.</p>"
                },
                "requirements": {
                    "value": ""
                },
                "rules": [],
                "slug": null,
                "source": {
