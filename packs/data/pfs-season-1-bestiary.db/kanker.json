        {
            "_id": "6LA2hDqLWNReVh4A",
            "data": {
                "ability": {
                    "value": ""
                },
                "area": {
                    "areaType": "",
                    "value": null
                },
                "areasize": {
                    "value": "4 contiguous 5-foot squares or Targets 1 object of 1 Bulk or less"
                },
                "category": {
                    "value": "spell"
                },
                "components": {
                    "material": false,
