            "img": "systems/pf2e/icons/spells/wall-of-ice.webp",
            "name": "Wall of Ice",
            "sort": 300000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": null,
                "category": {
                    "value": "spell"
                },
                "components": {
