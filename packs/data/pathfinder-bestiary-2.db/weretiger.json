                }
            },
            "effects": [],
            "flags": {
                "core": {
                    "sourceId": "Compendium.pf2e.bestiary-ability-glossary-srd.hFtNbo1LKYCoDy2O"
                }
            },
            "img": "systems/pf2e/icons/actions/Reaction.webp",
            "name": "Attack of Opportunity",
            "sort": 600000,
