        {
            "_id": "pmfr4MB1abUN5RbW",
            "data": {
                "actionCategory": {
                    "value": "offensive"
                },
                "actionType": {
                    "value": "action"
                },
                "actions": {
                    "value": 1
                },
                "description": {
                    "chat": "",
                    "unidentified": "",
                    "value": "<p><strong>Requirement</strong> The ghonhatine is adjacent to the corpse of a creature that died within the last hour; <strong>Effect</strong> The ghonhatine devours a chunk of the corpse. For 1 minute, the ghonhatine gains fast healing 5 and a [[/r +2 # status bonus to damage]] status bonus to damage rolls. It can gain these benefits from any given corpse only once.</p>"
                },
                "requirements": {
                    "value": ""
                },
                "rules": [],
                "slug": null,
