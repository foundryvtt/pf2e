            "img": "systems/pf2e/icons/equipment/weapons/specific-magic-weapons/stoneraiser-javelin.webp",
            "name": "Stoneraiser Javelin",
            "sort": 200000,
            "system": {
                "baseItem": "javelin",
                "bonus": {
