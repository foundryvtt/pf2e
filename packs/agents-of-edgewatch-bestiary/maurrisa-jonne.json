            "img": "icons/equipment/hand/gauntlet-simple-leather-brown-gold.webp",
            "name": "+1 Striking Handwraps of Mighty Blows",
            "sort": 100000,
            "system": {
                "baseItem": null,
                "bonus": {
                    "value": 1
                },
                "bonusDamage": {
