        {
            "_id": "8eumTeww6536avGC",
            "img": "systems/pf2e/icons/default-icons/action.svg",
            "name": "Halyard Swing",
            "sort": 400000,
            "system": {
                "actionType": {
                    "value": "action"
                },
                "actions": {
                    "value": 2
                },
                "category": "interaction",
                "description": {
                    "value": "<p><strong>Requirements</strong> The gargarthezul is on the deck of a ship.</p>\n<p><strong>Effect</strong> The gargarthezul swings from a nearby rope, moving up to twice its Speed and ignoring any terrain or lack of surface beneath it. It can make a tentacle Strike at any point during this movement.</p>"
                },
                "publication": {
