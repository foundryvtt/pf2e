                        ],
                        "selector": "survival",
                        "type": "circumstance",
                        "value": 2
                    },
                    {
                        "domain": "ranged-attack-roll",
                        "key": "RollOption",
                        "option": "ignore-range-penalty:2",
                        "predicate": [
                            "hunted-prey"
                        ]
                    },
                    {
