            "img": "systems/pf2e/icons/spells/chain-lightning.webp",
            "name": "Chain Lightning",
            "sort": 200000,
            "system": {
                "area": null,
                "cost": {
                    "value": ""
                },
                "counteraction": false,
                "damage": {
                    "0": {
                        "applyMod": false,
