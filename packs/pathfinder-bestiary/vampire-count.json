                        "exceptions": [
                            {
                                "definition": [
                                    "item:magical",
                                    {
                                        "or": [
                                            "damage:material:dawnsilver",
                                            "damage:material:silver"
                                        ]
                                    }
                                ],
                                "label": "PF2E.IWR.Custom.MagicalSilver"
                            }
                        ],
