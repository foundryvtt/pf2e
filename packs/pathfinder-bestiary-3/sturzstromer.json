            "img": "systems/pf2e/icons/spells/earthquake.webp",
            "name": "Earthquake",
            "sort": 200000,
            "system": {
                "area": {
                    "type": "burst",
                    "value": 60
                },
                "cost": {
                    "value": ""
                },
                "counteraction": false,
                "damage": {
