        {
            "_id": "7xEdZ59dIOpCIy03",
            "_stats": {
                "compendiumSource": "Compendium.pf2e.equipment-srd.Item.FNDq4NFSN0g2HKWO"
            },
            "img": "icons/equipment/hand/gauntlet-simple-leather-brown-gold.webp",
            "name": "+1 Striking Handwraps of Mighty Blows",
            "sort": 1100000,
            "system": {
                "baseItem": null,
                "bonus": {
                    "value": 1
                },
                "bonusDamage": {
                    "value": 0
