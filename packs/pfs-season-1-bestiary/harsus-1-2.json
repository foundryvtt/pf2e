                },
                "quantity": 1,
                "rules": [],
                "size": "med",
                "slug": "scroll-of-1st-level-spell",
                "spell": {
                    "_id": "i35dpZFI7jZcRoBo",
                    "flags": {
                        "core": {
                            "sourceId": "Compendium.pf2e.spells-srd.Item.i35dpZFI7jZcRoBo"
