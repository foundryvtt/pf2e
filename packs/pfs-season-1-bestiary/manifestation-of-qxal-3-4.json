            "img": "icons/magic/nature/root-vine-entangled-humanoid.webp",
            "name": "Wall of Thorns (At Will)",
            "sort": 200000,
            "system": {
                "area": null,
                "cost": {
                    "value": ""
                },
                "counteraction": false,
                "damage": {
                    "0": {
                        "applyMod": false,
