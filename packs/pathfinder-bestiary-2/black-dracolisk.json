        {
            "_id": "5aqMBttykiS0SKAb",
            "img": "systems/pf2e/icons/actions/Reaction.webp",
            "name": "Petrifying Glance",
            "sort": 500000,
            "system": {
                "actionType": {
                    "value": "reaction"
                },
                "actions": {
                    "value": null
                },
                "category": "defensive",
                "description": {
                    "value": "<p><strong>Trigger</strong> A creature within 30 feet that the dracolisk can see starts its turn</p>\n<hr />\n<p><strong>Effect</strong> The target must attempt a @Check[type:fortitude|dc:26] save. If it fails, it becomes @UUID[Compendium.pf2e.conditionitems.Item.Slowed]{Slowed 1} for 1 minute as its body stiffens.</p>"
                },
                "publication": {
                    "license": "OGL",
                    "remaster": false,
                    "title": ""
                },
                "rules": [],
                "slug": "petrifying-glance",
                "traits": {
