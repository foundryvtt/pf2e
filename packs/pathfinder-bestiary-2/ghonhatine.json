        {
            "_id": "pmfr4MB1abUN5RbW",
            "img": "systems/pf2e/icons/actions/OneAction.webp",
            "name": "Feed",
            "sort": 500000,
            "system": {
                "actionType": {
                    "value": "action"
                },
                "actions": {
                    "value": 1
                },
                "category": "offensive",
                "description": {
                    "value": "<p><strong>Requirements</strong> The ghonhatine is adjacent to the corpse of a creature that died within the last hour</p>\n<hr />\n<p><strong>Effect</strong> The ghonhatine devours a chunk of the corpse. For 1 minute, the ghonhatine gains fast healing 5 and a +2 status bonus to damage rolls. It can gain these benefits from any given corpse only once.</p>\n<p>@UUID[Compendium.pf2e.bestiary-effects.Item.Effect: Ghonhatine Feed]</p>"
                },
