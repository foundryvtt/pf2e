            "img": "systems/pf2e/icons/spells/wall-of-wind.webp",
            "name": "Wall of Wind",
            "sort": 200000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": null,
                "category": {
                    "value": "spell"
                },
                "components": {
                    "focus": false,
