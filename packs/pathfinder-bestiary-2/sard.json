            "img": "systems/pf2e/icons/spells/storm-of-vengeance.webp",
            "name": "Storm of Vengeance",
            "sort": 200000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": {
                    "type": "burst",
                    "value": 360
                },
                "category": {
