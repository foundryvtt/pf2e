            "img": "systems/pf2e/icons/spells/control-weather.webp",
            "name": "Control Weather (Doesn't require secondary casters)",
            "sort": 300000,
            "system": {
                "ability": {
                    "value": ""
                },
                "area": null,
                "category": {
                    "value": "ritual"
                },
                "components": {
                    "focus": false,
                    "material": false,
