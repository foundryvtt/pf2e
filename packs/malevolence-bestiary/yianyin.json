            "img": "systems/pf2e/icons/spells/phantasmal-killer.webp",
            "name": "Phantasmal Killer",
            "sort": 400000,
            "system": {
                "area": null,
                "cost": {
                    "value": ""
                },
                "counteraction": false,
                "damage": {
                    "0": {
                        "category": null,
